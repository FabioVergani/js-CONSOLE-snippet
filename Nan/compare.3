'37.37',
[],
[0],
'',
' ',
'       '
].map(v=>{
	var f=isNaN,x=v;
	if(f(x)){
		x=0;
	}else{
		if(f(x=parseInt(x))){
			x=v>>>0
		};
	};
	//x>>>0
	return  x+'//'+v+', floor:'+Math.floor(v);

}).join('\n'));

