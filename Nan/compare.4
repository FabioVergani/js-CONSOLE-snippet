/*
https://tc39.github.io/ecma262/#sec-tointeger
If number is NaN, return +0.
If number is +0, -0, +∞, or -∞, return number*
*the same sign as number and whose magnitude is floor(abs(number)).
*/


console.dir([
undefined,
NaN,
'NaN',
'blabla',
'   x    ',
0 / 0,
{},
[1,2,3],
false,
true,
null,
Infinity,
-Infinity,

-2.8,
-2.2,
-2,
-1.6,
-1.5,
-1.4,
-1,
.4,
.5,
.6,
1.4,
1.5,
1.6,
2.8,
37,
'37',
'37.37',
[],
[0],
'',
' ',
'       '
].map(v=>{
	var f=isNaN,x=v;
	if(f(x)){
		x=0;
	}else{
		//x=parseInt(x);
		x=Math.abs(x);
		if(f(x)){
			x=v>>>0
		}else{
			x=Math.floor(v);
		};
	};
	
	return  x+'//'+v+', floor:'+Math.floor(v>0?Math.abs(v):-Math.abs(v));

}).join('\n'));
